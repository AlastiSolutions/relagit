{
	"name": "relagit",
	"description": "Take back control over your git workflow.",
	"main": "./dist/main.js",
	"type": "module",
	"version": "0.0.1",
	"packageManager": "pnpm@8.8.0",
	"license": "RLPL",
	"scripts": {
		"dev": "rollup -c -w",
		"dev:once": "rollup -c",
		"build": "rollup -c",
		"start": "electron ./dist/main.js",
		"start:prod": "electron-forge start",
		"format": "prettier --write .",
		"lint": "eslint . --ext .js,.jsx,.ts,.tsx",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"make:mac": "electron-forge make --platform=darwin",
		"make:linux": "electron-forge make --platform=linux",
		"make:windows": "electron-forge make --platform=win32"
	},
	"author": "TheCommieAxolotl",
	"dependencies": {
		"vscode-oniguruma": "^1.7.0"
	},
	"devDependencies": {
		"@babel/core": "^7.23.0",
		"@babel/preset-env": "^7.22.20",
		"@babel/preset-typescript": "^7.23.0",
		"@electron-forge/cli": "^6.4.2",
		"@electron-forge/maker-deb": "^6.4.2",
		"@electron-forge/maker-dmg": "^6.4.2",
		"@electron-forge/maker-rpm": "^6.4.2",
		"@electron-forge/maker-squirrel": "^6.4.2",
		"@electron-forge/maker-zip": "^6.4.2",
		"@primer/octicons": "^18.3.0",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^24.1.0",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.2.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@rollup/plugin-terser": "^0.4.3",
		"@solid-devtools/overlay": "^0.4.3",
		"@solidjs/meta": "^0.28.6",
		"@solidjs/router": "^0.7.1",
		"@types/babel__core": "^7.20.2",
		"@types/babel__preset-env": "^7.9.3",
		"@types/node": "^18.18.0",
		"@types/primer__octicons": "^17.11.0",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"@wooorm/starry-night": "^2.2.0",
		"babel-preset-solid": "^1.7.12",
		"electron": "^25.8.4",
		"esbuild": "^0.17.19",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-editorconfig": "^4.0.3",
		"eslint-plugin-path-alias": "^1.0.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-unused-imports": "^2.0.0",
		"hast-util-to-html": "^9.0.0",
		"parse-git-diff": "^0.0.9",
		"prettier": "^3.0.3",
		"rollup": "^3.29.4",
		"rollup-plugin-esbuild": "^5.0.0",
		"rollup-plugin-scss": "^4.0.0",
		"rollup-plugin-serve": "^2.0.2",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"rollup-plugin-tsconfig-paths": "^1.5.1",
		"sass": "^1.68.0",
		"solid-js": "^1.7.12",
		"solid-transition-group": "^0.2.3",
