{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": "src",
		"target": "ESNext",
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"paths": {
			"@app/*": ["../../app/src/*"],
			"@ui/*": ["../../app/src/ui/*"],
			"@stores/*": ["../../app/src/stores/*"],
			"@modules/*": ["../../app/src/modules/*"],
			"~/*": ["../../*"]
		}
	}
}
